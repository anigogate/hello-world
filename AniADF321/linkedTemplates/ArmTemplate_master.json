{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AniADF321"
		},
		"AzureDatabricksLinkedService1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksLinkedService1'"
		},
		"AzureStorageLinkedService1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorageLinkedService1'"
		},
		"trigger1_properties_DatabrickParameterPipeline_parameters_query": {
			"type": "string",
			"defaultValue": "SELECT * FROM source LIMIT 8"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AniADF321_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricksLinkedService1_accessToken": {
						"value": "[parameters('AzureDatabricksLinkedService1_accessToken')]"
					},
					"AzureStorageLinkedService1_connectionString": {
						"value": "[parameters('AzureStorageLinkedService1_connectionString')]"
					},
					"trigger1_properties_DatabrickParameterPipeline_parameters_query": {
						"value": "[parameters('trigger1_properties_DatabrickParameterPipeline_parameters_query')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}